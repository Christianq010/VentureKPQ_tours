<!--The Subscriptions View that shows a table of all Emails from the Newsletter Database-->

<div class="container">

    <h1>Subscriptions</h1>
    <table class="table table-bordered table-striped">
        <thead>
        <tr>
            <th>ID</th>
            <th>E-mail</th>
        </tr>
        </thead>
        <tbody>
        {{#each model as |subscription|}}
            <tr>
                <th>{{subscription.id}}</th>
                <td>{{subscription.email}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>

</div>
{{outlet}}
